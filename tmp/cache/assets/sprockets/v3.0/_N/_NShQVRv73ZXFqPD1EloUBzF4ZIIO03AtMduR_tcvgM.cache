I"Ë(function() {
  jQuery(function() {
    var albumes;
    $('#song_album_id').parent().hide();
    albumes = $('#song_album_id').html();
    return $('#song_artist_id').change(function() {
      var artista, escaped_artist, options;
      artista = $('#song_artist_id :selected').text();
      escaped_artist = artista.replace(/([ #;&,.+*~\':"!^$[\]()=>|\/@])/g, '\\$1');
      options = $(albumes).filter("optgroup[label='" + escaped_artist + "']").html();
      if (options) {
        $('#song_album_id').html(options);
        return $('#song_album_id').parent().show();
      } else {
        $('#song_album_id').empty();
        return $('#song_album_id').parent().hide();
      }
    });
  });

}).call(this);
